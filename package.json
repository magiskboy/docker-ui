{
  "name": "docker-ui",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "rm -rf ./build && yarn build:server && concurrently \"vite\" \"node build/server.cjs\"",
    "build": "yarn build:server && yarn build:client",
    "build:client": "tsc -b && vite build",
    "build:server": "tsup",
    "start": "node build/server.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "gen-apiclient": "openapi-generator-cli generate -i openapi/v1.45.yaml -g typescript-axios -o ./src/api -p supportsES6=true,useSingleRequestParameter=true"
  },
  "dependencies": {
    "@tanstack/query-core": "^5.59.16",
    "@tanstack/react-router": "^1.76.1",
    "@xterm/addon-fit": "^0.10.0",
