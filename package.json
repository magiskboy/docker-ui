    "@xterm/xterm": "^5.5.0",
    "antd": "^5.21.5",
    "axios": "^1.7.7",
    "dayjs": "^1.11.13",
    "cors": "^2.8.5",
    "dockerode": "^4.0.2",
    "express": "^4.21.1",
    "http-proxy-middleware": "^3.0.3",
    "ws": "^8.18.0",
    "i18next": "^23.16.2",
    "i18next-http-backend": "^2.6.2",
    "jotai": "^2.10.1",
    "jotai-tanstack-query": "^0.9.0",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.1.0",
    "react-icons": "^5.3.0",
    "react-json-view": "^1.21.3",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@tanstack/router-devtools": "^1.76.1",
    "@tanstack/router-plugin": "^1.76.0",
    "@types/cors": "^2.8.17",
    "@types/dockerode": "^3.3.32",
    "@types/express": "^5.0.0",
    "@types/lodash": "^4.17.12",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@types/ws": "^8.5.13",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "concurrently": "^9.0.1",
    "eslint": "^9.13.0",
